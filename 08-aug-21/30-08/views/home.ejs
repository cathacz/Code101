<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <title>UserData</title>
    <style>
      table {
        border: 1px solid black;
      }
    </style>
  </head>
  <body>
    <form action="/" method="POST">
    <input type="submit" value="Add Data"></input></form>
    <%if(data.length){%>
        <table>
          <thead>
            <th>ID</th>
            <th>Avatar</th>
            <th>Name</th>
            <th>LName</th>
            <th>City</th>
          </thead>
          <tbody>
            <%for(let i=0;i< data.length;i++){%>
            <tr>
              <td><%=i%></td>
              <td><img src="<%=data[i].imageUrl%>" alt="Avatar" /></td>
              <td><%=data[i].firstName%></td>
              <td><%=data[i].lastName%></td>
              <td><%=data[i].city%></td>
            </tr>
            <%}%>
          </tbody>
        </table>
        <%}%>
    <%# comment%>
  </body>
</html>
